    "expression_rules": [
      {
        "uuid": "_a-0000eb16-753f-8000-9d01-011c48011c48_9545781",
        "name": "AS_GSS_groupsToBeupdatedForEvaluationChiefVisibility",
        "object_type": "Expression Rule",
        "description": "Groups to be updated for removing /adding the Evaluation Chief based on the AS_GSS_TOGGLE_EVALUATION_CHIEF_VISIBILITY",
        "data": {
          "sail_code": "a!localVariables(\n  local!groups: {\n    cons!AS_GSS_GRP_BUSINESS_ROLES,\n    cons!AS_GSS_GRP_CONTRACTING_PERSONNEL,\n    cons!AS_GSS_TMG_GRP_TASK_RECIPIENTS\n  },\n  local!isEvaluationChiefExists: #\"SYSTEM_SYSRULES_forEach\"(\n    local!groups,\n    contains(\n      togroup(\n        a!groupMembers(\n          group: fv!item,\n          direct: true,\n          memberType: \"GROUP\"\n        ).data\n      ),\n      cons!AS_GSS_GRP_EVALUATION_CHAIRS\n    )\n  ),\n  if(\n    cons!AS_GSS_TOGGLE_EVALUATION_CHIEF_VISIBILITY,\n    /*Returns the groups that doesn't contain Evaluation Chair to add when the toggle is on */\n    index(\n      local!groups,\n      where(not(local!isEvaluationChiefExists)),\n      null\n    ),\n    /*Returns the groups that contains Evaluation Chair to remove when the toggle is off*/\n    index(\n      local!groups,\n      where(local!isEvaluationChiefExists),\n      null\n    )\n  )\n)",
          "definition": "a!localVariables(\n  local!groups: {\n    cons!AS_GSS_GRP_BUSINESS_ROLES,\n    cons!AS_GSS_GRP_CONTRACTING_PERSONNEL,\n    cons!AS_GSS_TMG_GRP_TASK_RECIPIENTS\n  },\n  local!isEvaluationChiefExists: #\"SYSTEM_SYSRULES_forEach\"(\n    local!groups,\n    contains(\n      togroup(\n        a!groupMembers(\n          group: fv!item,\n          direct: true,\n          memberType: \"GROUP\"\n        ).data\n      ),\n      cons!AS_GSS_GRP_EVALUATION_CHAIRS\n    )\n  ),\n  if(\n    cons!AS_GSS_TOGGLE_EVALUATION_CHIEF_VISIBILITY,\n    /*Returns the groups that doesn't contain Evaluation Chair to add when the toggle is on */\n    index(\n      local!groups,\n      where(not(local!isEvaluationChiefExists)),\n      null\n    ),\n    /*Returns the groups that contains Evaluation Chair to remove when the toggle is off*/\n    index(\n      local!groups,\n      where(local!isEvaluationChiefExists),\n      null\n    )\n  )\n)",
